<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <id>SyncClient.SQLite</id>
    <version>3.8.4.3</version>
    <title>SyncClient SQLite Toolkit</title>
    <authors>Sébastien Pertus</authors>
    <owners>Sébastien Pertus</owners>
    <projectUrl>http://syncwinrt.codeplex.com</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package is the WinRT implementation of the Sync Framework Toolkit to enabled synchronization with WinRT 8.1 or Windows Phone 8 and SQLite.
      Installing package on Client WinRT will install Microsoft.Synchronization.ClientServices.Win8.dll.
      Installing package on Client WP8 will install Microsoft.Synchronization.ClientServices.WP8.dll
      Installing package on Server WebSite (Fx 35, 40, 45) will install Microsoft.Synchronization.Services.dll
    </description>
    <summary>Sync Toolkit for Windows Phone with SQLite.</summary>
    <tags>SyncFramework WinRT SQLite</tags>
	<dependencies>
		<group targetFramework="WindowsPhone8.0">
			<dependency id="SQLitePCL" version="3.8.4.2" />
		</group>
		<group targetFramework=".NETCore4.5">
			<dependency id="SQLitePCL" version="3.8.4.2" />
		</group>
	</dependencies>
    <references>
      <group targetFramework=".NETFramework3.5">
        <reference file="Microsoft.Synchronization.Services.dll" />
      </group>
      <group targetFramework="WindowsPhone8.0">
        <reference file="Microsoft.Synchronization.ClientServices.WP8.dll" />
      </group>
      <group targetFramework=".NETCore4.5">
        <reference file="Microsoft.Synchronization.ClientServices.Win8.dll" />
      </group>
    </references>
  </metadata>
</package>